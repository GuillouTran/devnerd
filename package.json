    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "6.2.0",
    "filesize": "6.1.0",
    "fs-extra": "9.0.1",
    "gzip-size": "5.1.1",
    "html-webpack-plugin": "4.5.0",
    "http-proxy-middleware": "^1.0.6",
    "jest": "26.6.3",
    "json-loader": "0.5.7",
    "node-sass": "^5.0.0",
    "object-assign": "4.1.1",
    "path-exists": "4.0.0",
    "postcss-loader": "4.0.4",
    "promise": "8.1.0",
    "react-dev-utils": "^11.0.0",
    "recursive-readdir": "2.2.2",
    "sass-loader": "^10.0.5",
    "strip-ansi": "6.0.0",
    "style-loader": "2.0.0",
    "url-loader": "4.1.1",
